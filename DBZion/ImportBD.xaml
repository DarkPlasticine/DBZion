<Window x:Class="DBZion.ImportBD"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DBZion"
        mc:Ignorable="d"
        Title="ImportBD" Height="300" Width="300">
    <Grid>
        <StackPanel>
            <TextBlock x:Name="txbPath"/>
            <Button Content="Путь" x:Name="btnPath" Click="btnPath_Click"/>
            <Button Content="Импортировать" x:Name="btnImport" Click="btnImport_Click"/>
        </StackPanel>
    </Grid>
</Window>
